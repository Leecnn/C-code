#include<stdio.h>
#include<math.h>
int main()
{
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    if(a+b>c&&a+c>b&&b+c>a)
    {
        if(a==b&&b==c)
        {
            printf("该三角形为等边三角形\n");
        }
        eles if(a=b||b=c||a=c)
        {
            printf("该三角形为等腰三角形\n");
        }
        eles if(a*a+b*b=c*c||a*a+c*c=b*b||c*c+b*b=a*a)
        {
            printf("该三角形为直角三角形\n");
        }
        eles
        {
            printf("该三角形为任意三角形\n");
        }
    }
    eles
    {
        printf("不能构成三角形\n");
    }
    return 0;
}
